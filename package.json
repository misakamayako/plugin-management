{
  "name": "plugin-management",
  "version": "1.0.0",
  "private": true,
  "config": {
    "components": "./src/components",
    "route": "./src/router",
    "dist": "./dist"
  },
  "scripts": {
    "create-component": "yo @jetbrains/ring-ui:react --path $npm_package_config_components",
    "build": "webpack --mode=production --devtool sourcemap",
    "test": "karma start",
    "lint": "eslint --fix . && npm run stylelint",
    "stylelint": "stylelint --ignore-path .gitignore **/*.css",
    "ci-test": "npm run lint && xvfb-maybe karma start --teamcity",
    "start": "webpack-dev-server -d --history-api-fallback --inline --hot --colors --port 9010"
  },
  "babel": {
    "presets": [
      [
        "@jetbrains/jetbrains",
        {
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@jetbrains/babel-preset-jetbrains": "^2.1.4",
    "@jetbrains/icons": "3.7.0",
    "@jetbrains/logos": "1.1.9",
    "@jetbrains/ring-ui": "3.0.13",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^2.6.11",
    "css-loader": "^3.5.3",
    "eslint-plugin-bdd": "^2.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.2.0",
    "json-loader": "^0.5.7",
    "less-loader": "^6.0.0",
    "normalize.css": "^8.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "@jetbrains/eslint-config": "^5.3.1",
    "@jetbrains/generator-ring-ui": "2.0.71",
    "@jetbrains/stylelint-config": "^2.0.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.8.2",
    "chai-enzyme": "1.0.0-beta.1",
    "electron": "^8.2.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "karma": "^6.3.16",
    "karma-chai-plugins": "^0.9.0",
    "karma-electron": "^6.3.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^1.1.0",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.1.2",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "stylelint": "^13.3.3",
    "xvfb-maybe": "^0.2.1",
    "yo": "^3.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
